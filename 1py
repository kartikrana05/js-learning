def solution(M, R, D):
    stock = M

    for day in range(D):
        if R == 0:
            return "YES"

        if stock > 0:
            stock -= 1
            R -= 1

        elif stock<M:
            stock+=1
    

    return "YES" if R == 0 else "NO"


M = int(input())
R = int(input())
D = int(input())

out_ = solution(M, R, D)
print(out_)